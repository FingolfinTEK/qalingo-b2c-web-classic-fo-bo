<header class="subhead" id="overview">
	<div class="navbar navbar-fixed-top">
		<div class="navbar-inner">
			
			<div class="container">
				
				<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</a>
				
				<a class="brand" href="$!common.homeUrl">$!appNameHtml</a>
				
				<div class="nav-collapse">

					<ul class="nav nav-pills market-nav">
						<li class="dropdown">
							<a class="dropdown-toggle" data-toggle="dropdown" href="#">MarketPlaces&nbsp;<b class="caret"></b></a>
							<ul class="dropdown-menu">
							#foreach($marketPlace in $marketPlaces)
								<li><a href="$!marketPlace.changeContextUrl" alt="$!marketPlace.name">$!marketPlace.name</a></li>
							#end
							</ul>
						</li>
						<li class="dropdown">
							<a class="dropdown-toggle" data-toggle="dropdown" href="#">Markets&nbsp;<b class="caret"></b></a>
							<ul class="dropdown-menu">
							#foreach($market in $markets)
								<li><a href="$!market.changeContextUrl" alt="$!market.name">$!market.name</a></li>
							#end
							</ul>
						</li>
						<li class="dropdown">
							<a class="dropdown-toggle" data-toggle="dropdown" href="#">Market Area&nbsp;<b class="caret"></b></a>
							<ul class="dropdown-menu">
							#foreach($marketArea in $marketAreas)
								<li><a href="$!marketArea.changeContextUrl" alt="$!marketArea.name">$!marketArea.name</a></li>
							#end
							</ul>
						</li>
						<li class="dropdown">
							<a class="dropdown-toggle" data-toggle="dropdown" href="#">M. Area Langues&nbsp;<b class="caret"></b></a>
							<ul class="dropdown-menu">
							#foreach($marketAreaLocalization in $marketAreaLocalizations)
								<li><a href="$!marketAreaLocalization.changeContextUrl" alt="$!marketAreaLocalization.name">$!marketAreaLocalization.name</a></li>
							#end
							</ul>
						</li>
						<li class="dropdown">
							<a class="dropdown-toggle" data-toggle="dropdown" href="#">M. Area Retailers&nbsp;<b class="caret"></b></a>
							<ul class="dropdown-menu">
							#foreach($marketAreaRetailer in $marketAreaRetailers)
								<li><a href="$!marketAreaRetailer.changeContextUrl" alt="$!marketAreaRetailer.name">$!marketAreaRetailer.name</a></li>
							#end
							</ul>
						</li>
					</ul>

					<div style="float: right;">
						<ul class="nav pull-right">
							<li class="dropdown">
								
								<a href="#" class="dropdown-toggle" data-toggle="dropdown">
									<i class="icon-cog"></i>
									Settings
									<b class="caret"></b>
								</a>

								
								<ul class="dropdown-menu">
									<li><a href="$!common.personalDetailsUrl" alt="Account Settings">Account Settings</a></li>
									<li><a href="$!common.personalDetailsUrl" alt="Privacy Settings">Privacy Settings</a></li>
									<li class="divider"></li>
									<li><a href="https://github.com/qalingo/qalingo-b2c-engine/wiki" alt="Wiki" target="_blank">Wiki</a></li>
								</ul>
								
							</li>

							#if($!user)
							<li class="dropdown">								
								<a href="#" class="dropdown-toggle" data-toggle="dropdown">
									<i class="icon-user"></i> 
									$!user.lastname $!user.firstname
									<b class="caret"></b>
								</a>
								<ul class="dropdown-menu">
									<li><a href="$!common.personalDetailsUrl">My Profile</a></li>
									<li><a href="$!common.personalDetailsUrl">My Groups</a></li>

									<li class="divider"></li>
									<li><a href="$!common.logoutUrl">$wording.get("bo.auth.header_title_logout")</a></li>
								</ul>
							</li>
							#end
						</ul>
					</div>
				</div>
				<div id="app-env" style="clear: both;">
					<div>
						<div style="float: left; margin-top: 8px; margin-bottom: 5px;">
							$envName
						</div>
						<ul class="nav nav-pills market-nav">
							<li class="dropdown">
								<a class="dropdown-toggle" data-toggle="dropdown" href="#">Backoffice Langues&nbsp;<b class="caret"></b></a>
								<ul class="dropdown-menu">
								#foreach($localization in $backofficeLocalizations)
									<li><a href="$!localization.url" alt="$!localization.name">$!localization.name</a></li>
								#end
								</ul>
							</li>
						</ul>
					</div>
					<div style="float: right;">
						<form action="$!quickSearch.urlFormSubmit" class="navbar-search pull-right" method="POST">
							<input id="global-search-txt" name="global-search-txt" type="text" class="search-query" placeholder="$wording.get("bo.search.form_search_label_text")">
						</form>
					</div>
				</div>
				<div style="clear: both; color: #7D7E81;">
					Marketplace: <span style="color: #a1cd44;">$!common.currentMarketPlace.name</span>
					Market: <span style="color: #a1cd44;">$!common.currentMarket.name</span>
					MarketArea: <span style="color: #a1cd44;">$!common.currentMarketArea.name</span>
					M. Area Localization: <span style="color: #a1cd44;">$!common.currentMarketAreaLocalization.name</span>
					M. Area Retailer: <span style="color: #a1cd44;">$!common.currentMarketAreaRetailer.name</span>
				</div>
			</div>
		</div>
	</div>
</header>